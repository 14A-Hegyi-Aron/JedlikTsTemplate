{
    "name": "jedlik-typescript-vscode-template",
    "version": "1.0.0",
    "type": "module",
    "description": "Jedlik project template for using TypeScript and Node.js in VS Code.",
    "engines": {
        "node": "10.x"
    },
    "main": "app.js",
    "repository": {
        "private": true,
        "type": "git",
        "url": "https://github.com/nitslaszlo/JedlikTsTemplate"
    },
    "scripts": {
        "start": "node ./dist/app.js",
        "start-debug": "node --debug=5858 ./dist/app.js",
        "install:nodemodules": "npm install",
        "compile": "tsc",
        "postinstall": "npm run compile",
        "kill": "taskkill /im node.exe /F",
        "test": "ts-node-dev --inspect --clear --respawn --transpileOnly ./src/app.ts"
    },
    "author": "Nits László",
    "license": "MIT",
    "dependencies": {
        "tslib": "^1.10.0"
    },
    "devDependencies": {
        "@types/node": "^13.1.7",
        "@typescript-eslint/eslint-plugin": "^2.16.0",
        "@typescript-eslint/parser": "^2.16.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-prettier": "^3.1.2",
        "prettier": "^1.19.1",
        "ts-node-dev": "^1.0.0-pre.44",
        "typescript": "^3.7.4"
    }
}
